<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.himedia.ecommerce.seller.mybatis.login.Login">
	
	<select id="loginProc" parameterType="java.lang.String" resultType="sellerDto">
		SELECT
			ID
			, PASSWD
		FROM
			TB_SLL
		WHERE
			ID = #{id}
	</select>
	
</mapper>